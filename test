<body>
    <div class="container">
        <div class="content-wrapper">
            <div class="logo">venoxware</div>
            <div class="key-container">
                <div class="key-text">${key}</div>
            </div>
            <div class="info">Changes every day at 9PM CET</div>
            <div class="button-group">
                <button class="button secondary-button" onclick="copyKey()">Copy Key</button>
                <button class="button primary-button" onclick="joinDiscord()">Get Script</button>
            </div>
        </div>
    </div>

    <script>
        document.addEventListener('contextmenu', function(event) {
            event.preventDefault();
        });

        function createSnowflake() {
            const snowflake = document.createElement('div');
            snowflake.classList.add('snowflake');
            snowflake.style.left = Math.random() * 100 + 'vw';
            snowflake.style.opacity = Math.random() * 0.6 + 0.2;
            snowflake.style.fontSize = (Math.random() * 12 + 8) + 'px';
            
            const snowflakes = ['❄', '•', '∗', '⋆', '✧'];
            snowflake.innerHTML = snowflakes[Math.floor(Math.random() * snowflakes.length)];
            
            const duration = (Math.random() * 3 + 3);
            const delay = Math.random() * 2;
            snowflake.style.animationDuration = duration + 's';
            snowflake.style.animationDelay = delay + 's';
            
            snowflake.style.transform = `translateX(${Math.random() * 10 - 5}px)`;
            
            document.body.appendChild(snowflake);

            snowflake.addEventListener('animationend', () => {
                snowflake.remove();
            });
        }

        setInterval(createSnowflake, 50);

        function copyKey() {
            const keyText = document.querySelector('.key-text').textContent;
            navigator.clipboard.writeText(keyText)
                .then(() => {
                    const button = document.querySelector('.secondary-button');
                    button.textContent = 'Copied!';
                    setTimeout(() => {
                        button.textContent = 'Copy Key';
                    }, 2000);
                });
        }

        function joinDiscord() {
            window.open('https://discord.gg/venoxhub', '_blank');
        }
    </script>
</body>
</html>
